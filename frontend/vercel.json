{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "name": "skycamos-frontend",
    "buildCommand": null,
    "outputDirectory": ".",
    "public": true,
    "trailingSlash": false,
    "cleanUrls": true,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        },
        {
            "source": "/sw.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache"
                },
                {
                    "key": "Service-Worker-Allowed",
                    "value": "/"
                }
            ]
        },
        {
            "source": "/manifest.json",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/manifest+json"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/((?!api|src|public|icons|styles|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}
